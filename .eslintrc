{
  "env": {
    "node": true,
    "es2021": true,
    "jquery": true,
    "browser": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error", {}, { "usePrettierrc": true }],
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off"
  }
}

